{
    "rules": {

        "no-alert": 1,
        "no-caller": 1,
        "no-bitwise": 0,
        "no-console": 1,
        "no-dangle": 1,
        "no-debugger": 1,
        "no-empty": 1,
        "no-eval": 1,
        "no-floating-decimal": 0,
        "no-implied-eval": 1,
        "no-with": 1,
        "no-fallthrough": 1,
        "no-unreachable": 1,
        "no-undef-init": 1,
        "no-octal": 1,
        "no-new-wrappers": 1,
        "no-new": 1,
        "no-new-func": 1,

        "smarter-eqeqeq": 0,
        "brace-style": 0,
        "camelcase": 1,
        "curly": 1,
        "dot-notation": 1,
        "eqeqeq": 1,
        "new-parens": 1,
        "guard-for-in": 0,
        "new-cap": 1,
        "quote-props": 0,
        "semi": 1,
        "use-isnan": 1

    }
}
