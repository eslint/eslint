    "escape-string-regexp": "^1.0.2",
    "escope": "^3.0.1",
    "espree": "^2.0.1",
    "estraverse": "^2.0.0",
    "estraverse-fb": "^1.3.1",
    "globals": "^6.1.0",
    "inquirer": "^0.8.2",
    "js-yaml": "^3.2.5",
    "minimatch": "^2.0.1",
    "mkdirp": "^0.5.0",
    "object-assign": "^2.0.0",
    "optionator": "^0.5.0",
    "path-is-absolute": "^1.0.0",
